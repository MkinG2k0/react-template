{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/default/TemplateName.tsx",
        "style": "templates/default/TemplateName.module.scss",
        "test": "templates/default/TemplateName.test.tsx"
      },
      "path": "src/Components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/page/TemplateName.tsx",
        "style": "templates/page/TemplateName.module.scss"
      },
      "path": "src/Page",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": true
    },
    "slice": {
      "customTemplates": {
        "component": "templates/slice/TemplateName.Slice.ts"
      },
      "path": "src/Redux/Slices",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}
